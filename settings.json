{
  "SeeDocsAt": "https://github.com/Microsoft/AirSim/blob/main/docs/settings.md",
  "SettingsVersion": 1.2,
  
  "SimMode": "Multirotor",
  "ClockSpeed": 1.0,
  
  "PhysicsEngineName": "FastPhysicsEngine",
  
  "CameraDefaults": {
    "CaptureSettings": [
      {
        "ImageType": 0,
        "Width": 320,
        "Height": 240,
        "FOV_Degrees": 90,
        "AutoExposureSpeed": 100,
        "MotionBlurAmount": 0
      },
      {
        "ImageType": 2,
        "Width": 320,
        "Height": 240,
        "FOV_Degrees": 90
      },
      {
        "ImageType": 5,
        "Width": 320,
        "Height": 240,
        "FOV_Degrees": 90
      }
    ]
  },
  
  "Vehicles": {
    "Drone1": {
      "VehicleType": "SimpleFlight",
      "X": 0, "Y": 0, "Z": 0,
      "Yaw": 0,
      
      "AllowAPIAlways": true,
      
      "Cameras": {
        "front_center": {
          "CameraName": "front_center",
          "X": 0.25, "Y": 0.0, "Z": 0.0,
          "Pitch": 0, "Roll": 0, "Yaw": 0,
          "CaptureSettings": [
            {
              "ImageType": 0,
              "Width": 320,
              "Height": 240,
              "FOV_Degrees": 90
            },
            {
              "ImageType": 2,
              "Width": 320,
              "Height": 240,
              "FOV_Degrees": 90
            }
          ]
        }
      },
      
      "Sensors": {
        "Imu": {
          "SensorType": 2,
          "Enabled": true
        },
        "Gps": {
          "SensorType": 3,
          "Enabled": true
        },
        "Magnetometer": {
          "SensorType": 4,
          "Enabled": true
        },
        "Barometer": {
          "SensorType": 1,
          "Enabled": true
        },
        "Lidar1": {
          "SensorType": 6,
          "Enabled": true,
          "NumberOfChannels": 16,
          "Range": 40,
          "PointsPerSecond": 10000,
          "RotationsPerSecond": 10,
          "VerticalFOVUpper": 15,
          "VerticalFOVLower": -15,
          "HorizontalFOVStart": -180,
          "HorizontalFOVEnd": 180,
          "DrawDebugPoints": false,
          "X": 0, "Y": 0, "Z": -0.1
        }
      }
    }
  },
  
  "Recording": {
    "RecordOnMove": false,
    "RecordInterval": 0.05,
    "Cameras": [
      {"CameraName": "front_center", "ImageType": 0, "PixelsAsFloat": false, "Compress": true},
      {"CameraName": "front_center", "ImageType": 2, "PixelsAsFloat": true, "Compress": false}
    ]
  },
  
  "ViewMode": "NoDisplay",
  
  "SegmentationSettings": {
    "InitMethod": "CommonObjectsRandomIDs",
    "MeshNamingMethod": "OwnerName"
  }
}
